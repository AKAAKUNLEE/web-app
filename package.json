{
  "name": "web-app",
  "version": "1.0.0",
  "description": "A web app packaged with Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64 --publish never"
  },
  "build": {
    "appId": "com.example.Alist",
    "productName": "Alist",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable",
      "icon": "assets/icon.ico"
    },
    "extraResources": [
      {
        "from": "extensions",
        "to": "extensions"
      },
      {
        "from": "assets",
        "to": "assets"
      }
    ]
  },
  "dependencies": {
    "electron": "^latest"
  },
  "devDependencies": {
    "electron-builder": "^latest"
  }
}
