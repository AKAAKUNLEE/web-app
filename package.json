{
  "name": "web-app",
  "version": "1.0.0",
  "description": "A web app packaged with Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64 --publish never"
  },
  "build": {
    "appId": "com.example.Alist",
    "productName": "Alist",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable"
    },
    "extraResources": [
      {
        "from": "data",       
        "to": "data"          
      },
      {
        "from": "extensions",
        "to": "extensions"  
      }

    ]

  },
  "devDependencies": {
    "electron": "^35.1.3",
    "electron-builder": "^26.0.12"
  }
}
